<?xml version="1.0"?>
<extension key="nz.co.fuzion.dedupegroup" type="module">
  <file>dedupegroup</file>
  <name>Group Duplicate Contacts</name>
  <description>Adds duplicate contacts to group. If a group is selected for dedupe, ext will list the row only when contact A and B are both part of the group.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jitendra Purohit</author>
    <email>jitendra@fuzion.co.nz</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-02-16</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Dedupegroup</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
</extension>
